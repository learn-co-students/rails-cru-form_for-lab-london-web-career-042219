<div class="container">
  <br>
  <h1 class="text-success">Add a New Artist: </h1>
    <div class="text-danger">
      <%= form_for @artist do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <br>
      <%= f.label :bio %>
      <%= f.text_area :bio %>
      <br>
      <%=  f.submit class: "btn btn-info" %>
      <% end %>
    </div>
    <br>
    <div class="btn btn-warning"><%= link_to "Main Menu", artists_path %></div>
</div>
